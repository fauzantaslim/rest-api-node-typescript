    "husky": "^9.1.7",
    "prettier": "3.5.0",
    "pretty-quick": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  },
  "dependencies": {
    "express": "^4.21.2"
